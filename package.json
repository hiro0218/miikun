{
  "name": "Miikun",
  "version": "1.2.0",
  "description": "Markdown Editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build:js && npm run build:css && npm run build:vendor",
    "build:js": "webpack -p --progress",
    "build:css": "node ./task/style.js",
    "build:vendor": "node ./task/vendor.js",
    "dest": "node ./task/dest.js",
    "package": "npm run build && node ./task/electron.js"
  },
  "config": {
    "manifest": "./task/manifest.json",
    "style": "./renderer/scss/style.scss",
    "script": "./renderer/js/script.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hiro0218/editor.git"
  },
  "keywords": [
    "electron-app",
    "electron",
    "Markdown",
    "Markdown Editor",
    "Editor"
  ],
  "author": "hiro",
  "copyright": "Copyright (c) 2016 hiro. All rights reserved.",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "clean-css": "^3.4.19",
    "cpx": "^1.3.2",
    "css-loader": "^0.23.1",
    "devtron": "^1.3.0",
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.3.2",
    "github-markdown-css": "^2.3.0",
    "material-design-icons": "^2.2.3",
    "material-design-lite": "^1.1.3",
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "node-sass": "^3.8.0",
    "normalize.css": "^4.2.0",
    "uglify-js": "^2.7.0",
    "vue-hot-reload-api": "^2.0.5",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "codemirror": "^5.17.0",
    "codemirror-textlint": "^1.1.0",
    "markdown-it": "^7.0.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-footnote": "^3.0.0",
    "markdown-pdf": "^7.0.0",
    "nprogress": "^0.2.0",
    "textlint-rule-incremental-headers": "^0.2.0",
    "textlint-rule-max-appearence-count-of-words": "^1.0.1",
    "textlint-rule-max-ten": "^2.0.1",
    "textlint-rule-no-double-negative-ja": "^1.0.4",
    "textlint-rule-no-doubled-conjunction": "^1.0.2",
    "textlint-rule-no-doubled-conjunctive-particle-ga": "^1.0.2",
    "textlint-rule-no-doubled-joshi": "^3.3.0",
    "textlint-rule-no-mix-dearu-desumasu": "^3.0.2",
    "vue": "^1.0.26",
    "vue-electron": "^1.0.4",
    "vue-mdl": "^0.9.0",
    "vue-router": "^0.7.13"
  }
}
